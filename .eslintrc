{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "globals": {
    "window": true,
    "browser": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "no-console": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "complexity": [2,10],
    "curly": [1, "all"],
    "default-case": 2,
    "dot-notation": [0, {"allowKeywords":true}],
    "eqeqeq": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-octal": 2,
    "no-redeclare": 2,
    "no-warning-comments": [0, {"terms":["todo","fixme","xxx"],"location":"start"}],
    "no-delete-var": 2,
    "no-undef": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-mixed-requires": [0, true],
    "array-bracket-spacing": [2,"never",{}],
    "brace-style": [2,"1tbs",{}],
    "camelcase": [2,{"properties":"always"}],
    "comma-style": 2,
    "consistent-this": [2, "that,self"],
    "func-style": [0, "declaration"],
    "key-spacing": [0, {"beforeColon":false,"afterColon":true}],
    "max-nested-callbacks": [0, 2],
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [0, {"max":2}],
    "no-trailing-spaces": 2,
    "object-curly-spacing": [0, "never"],
    "quotes": [2,"single"],
    "semi-spacing": [2, {"after":true}],
    "space-after-keywords": [0, "always"],
    "space-before-blocks": [0, "always"],
    "space-before-function-paren": [2, "never"],
    "space-unary-ops": [0, {"words":true, "nonwords": true}],
    "max-depth": [0, 4],
    "max-len": [0, 2, 120,{"ignoreComments":true,"ignoreUrls":true}],
    "max-params": [0, 3],
    "max-statements": [0, 10],
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error"
  }
}